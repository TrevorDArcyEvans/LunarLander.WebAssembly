    <_ContentIncludedByDefault Remove="Blazor.Console\Demo\Console.Demo\wwwroot\sample-data\weather.json" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Blazor.Console\**" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="Blazor.Console\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Blazor.Console\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="Blazor.Console\**" />
  </ItemGroup>

</Project>
