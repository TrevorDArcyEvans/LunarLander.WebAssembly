    <_ContentIncludedByDefault Remove="Blazor.Console\Console.Extensions.Blazor\obj\Blzr.Console.csproj.nuget.dgspec.json" />
    <_ContentIncludedByDefault Remove="Blazor.Console\Console.Extensions.Blazor\obj\Debug\net6.0\staticwebassets.build.json" />
